@require 'util'

p
    margin-top 0
    margin-left 0
    margin-right 0
    margin-bottom 2px

.pane
    padding 5px

#canvas_div
    position relative

canvas,#canvasText
    position absolute
    left 0
    top 0 
    width 100%
    height 100%

#tileCanvas
    z-index 0
#entityCanvas
    z-index 1
#uiCanvas
    z-index 2
#canvasText
    z-index 3
