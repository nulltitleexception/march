{
  "name": "gotg",
  "version": "0.12.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "push": "npm run validate && git push",
    "prebuild-dist-html": "pug -O '{\"jquery_path\":\"https://code.jquery.com/jquery-3.4.1.min.js\", \"vue_path\":\"https://cdn.jsdelivr.net/npm/vue\", \"socket_io_path\":\"https://gotg.io/socket.io/socket.io.js\", \"github_link\": \"https://github.com/phi-fell/march\", \"bug_link\": \"https://github.com/phi-fell/march/issues\"}' -o site/html site/pug",
    "build-dist-html": "cpy . ../../dist --cwd=site/html --parents",
    "build-dist-js": "cpy . ../../dist/js --cwd=site/js --parents",
    "build-dist-css": "cpy . ../../dist/css --cwd=site/css --parents",
    "build-dist-logo": "cpy site/logo/favicon.ico dist",
    "build-dist-tex": "cpy . ../../dist/tex --cwd=site/tex --parents",
    "build-dist-octicons": "node build/build_octicons.js",
    "prebuild-dist": "npm run build && npm run build-dist-html && npm run build-dist-js && npm run build-dist-css && npm run build-dist-logo && npm run build-dist-tex && npm run build-dist-octicons",
    "build-dist": "zip -r -q dist.zip dist",
    "build-html": "pug -O '{\"jquery_path\":\"https://code.jquery.com/jquery-3.4.1.min.js\", \"vue_path\":\"https://cdn.jsdelivr.net/npm/vue\", \"socket_io_path\":\"https://gotg.io/socket.io/socket.io.js\", \"github_link\": \"https://github.com/phi-fell/march\", \"bug_link\": \"https://github.com/phi-fell/march/issues\"}' -o site/html site/pug",
    "build-styles": "stylus -c ./site/stylus --out ./site/css",
    "build-client-js": "tsc -p ./site",
    "build-client": "npm run build-client-js && npm run build-styles && npm run build-html",
    "build-server": "tsc",
    "build": " npm run build-server && npm run build-client",
    "predev": "npm run build-server && npm run build-client-js",
    "dev": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "devp": "node --prof build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "predev_static": "npm run build",
    "dev_static": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS -STATIC_SITE",
    "dev_no_build": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "devv": "npm run validate && npm run dev_no_build",
    "predebug": "npm run predev",
    "debug": "node --inspect build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "preprod": "npm run build",
    "prod": "node build/watcher.js -PUBLISH_DIAGNOSTIC_DATA -STATIC_SITE",
    "validate": "npm run lint && npm run build && npm run build-styles && npm run tests",
    "lint": "tslint -p '.' -c tslint.json 'src/**/*.ts'",
    "tests": "jest",
    "test": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nulltitleexception/march.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nulltitleexception/march/issues"
  },
  "homepage": "https://github.com/nulltitleexception/march#readme",
  "dependencies": {
    "@primer/octicons": "^9.6.0",
    "@types/async": "^3.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bent": "^7.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/jquery": "^3.3.34",
    "@types/pug": "^2.0.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/stylus": "^0.48.32",
    "@types/uuid": "^7.0.2",
    "async": "^3.2.0",
    "bcrypt": "^4.0.1",
    "bent": "^7.1.2",
    "cookie-parser": "^1.4.5",
    "cpy-cli": "^3.1.0",
    "express": "^4.17.1",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.3",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "socket.io": "^2.3.0",
    "stylus": "^0.54.7",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "tslint-consistent-codestyle": "^1.16.0",
    "vrsource-tslint-rules": "^6.0.0"
  }
}
