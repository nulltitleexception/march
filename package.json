{
  "name": "gotg",
  "version": "0.10.10",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p '.' -c tslint.json 'src/**/*.ts'",
    "build-browser-js": "tsc -p ./site",
    "build-styles": "stylus -c ./site/stylus --out ./site/css",
    "build-client": "npm run build-styles && npm run build-browser-js",
    "build-server": "tsc",
    "build": "npm run build-client && npm run build-server",
    "predev": "npm run build",
    "dev": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC",
    "dev_no_build": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC",
    "validate": "npm run lint && npm run build && npm run test",
    "devv": "npm run validate && npm run dev_no_build",
    "push": "npm run validate && git push",
    "predebug": "npm run predev",
    "debug": "node --inspect build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC",
    "preprod": "npm run build",
    "prod": "node build/watcher.js -PUBLISH_DIAGNOSTIC_DATA",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nulltitleexception/march.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nulltitleexception/march/issues"
  },
  "homepage": "https://github.com/nulltitleexception/march#readme",
  "dependencies": {
    "@types/async": "^3.0.7",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.0",
    "@types/jquery": "^3.3.31",
    "@types/pug": "^2.0.4",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
    "async": "^3.1.1",
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "fp-ts": "^2.4.4",
    "io-ts": "^2.1.0",
    "pug": "^2.0.4",
    "socket.io": "^2.2.0",
    "stylus": "^0.54.7",
    "typescript": "^3.7.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.18.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "vrsource-tslint-rules": "^6.0.0"
  }
}
