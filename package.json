  "scripts": {
    "lint": "tslint -p '.' -c tslint.json 'src/**/*.ts'",
    "build-client": "tsc -p ./site",
    "build-server": "tsc",
    "build": " npm run build-server && npm run build-client",
    "predev": "npm run build",
    "dev": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "dev_no_build": "node build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "validate": "npm run lint && npm run build && npm run tests",
    "devv": "npm run validate && npm run dev_no_build",
    "push": "npm run validate && git push",
    "predebug": "npm run predev",
    "debug": "node --inspect build/index.js -NO_HTTPS -PUBLISH_DIAGNOSTIC_DATA -UNLOCK_DIAGNOSTIC -JS_DEBUG_LIBS",
    "preprod": "npm run build",
    "prod": "node build/watcher.js -PUBLISH_DIAGNOSTIC_DATA",
    "tests": "jest",
    "test": "npm run validate"
